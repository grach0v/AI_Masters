malen = int(input())
words = []
for i in range(n):
    words.append(input())

for i in range(n):
    words[i] = words[i], ''.join(sorted(words[i]))

words.sort(key=lambda _, b: b)

i = 0
while i < n:
    print(words[i], end=' ')
    if i + 1 < n and words[i + 1] != words[i]:
        print('')

